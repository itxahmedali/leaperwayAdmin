import { ElementRef, OnDestroy, OnInit } from '@angular/core';
import { NgxEditorService } from '../../../editor.service';
import { SharedService } from '../../../services/shared/shared.service';
export declare class DropdownComponent implements OnInit, OnDestroy {
    private ngxeService;
    private sharedService;
    private el;
    private editorView;
    private pluginUpdateSubscription;
    group: any;
    items: any;
    isDropdownOpen: boolean;
    selected: string;
    private activeItems;
    disabledItems: any[];
    activeItem: string | null;
    constructor(ngxeService: NgxEditorService, sharedService: SharedService, el: ElementRef);
    get isSelected(): boolean;
    get isDropdownDisabled(): boolean;
    onDocumentClick(target: Node): void;
    getName(key: string): string;
    toggleDropdown(e: MouseEvent): void;
    onClick(e: MouseEvent, item: string): void;
    private update;
    ngOnInit(): void;
    ngOnDestroy(): void;
}
